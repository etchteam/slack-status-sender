{
  "name": "statusify-plugin",
  "version": "0.0.1",
  "description":
    "React plugin for 'Statusify', a service to display your Slack status on your website",
  "main": "./lib/index.js",
  "scripts": {
    "start": "babel-node ./node_modules/.bin/webpack-dev-server",
    "clean": "rimraf lib",
    "test":
      "mocha --require babel-core/register --require babel-polyfill --require ./test/testHelper.js --recursive && npm run lint",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:watch": "npm test -- --watch",
    "cover":
      "istanbul cover _mocha --report lcovonly -- --require babel-core/register --require babel-polyfill --require ./test/testHelper.js --recursive && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish":
      "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": ["lib", "src"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etchteam/statusify-plugin.git"
  },
  "keywords": ["boilerplate", "npm", "module", "node", "javascript"],
  "author": "Etch Software Ltd <hey@etch.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/etchteam/statusify-plugin/issues"
  },
  "homepage": "https://github.com/etchteam/statusify-plugin#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "babel-preset-react": "6.24.1",
    "chai": "^4.1.2",
    "codacy-coverage": "3.0.0",
    "cross-env": "5.1.5",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "fetch-mock": "6.3.0",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "11.10.0",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "1.3.0",
    "path": "0.12.7",
    "rimraf": "^2.6.2",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  }
}
